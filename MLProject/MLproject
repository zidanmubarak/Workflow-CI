name: Climate_Change_Agriculture_ML

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_path: {type: string, default: "climate_change_preprocessing.csv"}
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
      test_size: {type: float, default: 0.2}
      random_state: {type: int, default: 42}
    command: "python modelling.py --data-path {data_path} --n-estimators {n_estimators} --max-depth {max_depth} --test-size {test_size} --random-state {random_state}"
